<hibernate-mapping>
    <class name="org.leyla.bean.History" table="HISTORY">

        <id name="id" type="int" column="ID">
            <generator class="increment"/>
        </id>

        <property name="product" column="PRODUCT" type="string"/>

        <property name="market" column="MARKET" type="string"/>

    </class>
</hibernate-mapping>